class complex:
    def __init__(self , real , img):
        self.real = real
        self.img = img

    def shownumber(self):
        print(f"{self.real}i + {self.img}j") 

    def __add__(self , n2):
        newreal = self.real + n2.real
        newimg = self.img + n2.img
        return complex(newreal , newimg)
  
n1 = complex(3 , 3) 
n1.shownumber()  
n2 = complex(5 , 10)
n2.shownumber()

num3 = n1 + n2
num3.shownumber()

